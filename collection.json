            "name": "Get User by ID",
            "request": { "method": "GET", "header": [], "url": "http://localhost:3000/api/v1/users/:id" }
          },
          {
            "name": "Create User",
            "request": {
              "method": "POST",
              "header": [{ "key": "Content-Type", "value": "application/json" }],
              "body": { "mode": "raw", "raw": "{ \"name\": \"John Doe\", \"email\": \"john@example.com\" }" },
              "url": "http://localhost:3000/api/v1/users"
            }
          },
          {
            "name": "Update User",
            "request": {
              "method": "PUT",
              "header": [{ "key": "Content-Type", "value": "application/json" }],
              "body": { "mode": "raw", "raw": "{ \"name\": \"Jane Doe\" }" },
              "url": "http://localhost:3000/api/v1/users/:id"
            }
          },
          {
            "name": "Delete User",
            "request": { "method": "DELETE", "header": [], "url": "http://localhost:3000/api/v1/users/:id" }
